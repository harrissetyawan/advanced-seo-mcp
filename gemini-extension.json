{
  "name": "seobanget",
  "version": "0.1.1",
  "description": "Professional SEO suite: On-Page, Technical, Ahrefs Data, Speed Tests and Reporting",
  "mcpServers": [
    {
      "id": "advanced-seo-mcp",
      "command": "uv",
      "args": [
        "run",
        "--with", "fastmcp>=2.14.2",
        "--with", "requests>=2.32.5",
        "--with", "beautifulsoup4>=4.14.3",
        "--with", "lxml>=6.0.2",
        "--with", "fake-useragent>=2.2.0",
        "--with", "python-dotenv>=1.0.0",
        "-m",
        "advanced_seo_mcp.server"
      ],
      "env": {
        "PYTHONPATH": "src"
      }
    }
  ]
}
